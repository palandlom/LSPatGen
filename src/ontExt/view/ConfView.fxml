<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="700.0" minWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ontExt.view.ConfController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <center>
            <TableView fx:id="confTable" editable="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="propertyCol" maxWidth="1.7976931348623157E308" minWidth="60.0" prefWidth="340.0" text="Property" />
                <TableColumn fx:id="valueCol" maxWidth="1.7976931348623157E308" minWidth="300.0" onEditCommit="#handleValueCellEdit" prefWidth="548.0" text="Value" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </TableView>
         </center>
         <bottom>
            <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="20.0" BorderPane.alignment="CENTER_RIGHT">
               <children>
                  <Button fx:id="backButton" minWidth="80.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
                  <Button fx:id="saveButton" disable="true" minWidth="80.0" mnemonicParsing="false" onAction="#handleSaveButton" text="Save" />
               </children>
               <padding>
                  <Insets top="40.0" />
               </padding>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
